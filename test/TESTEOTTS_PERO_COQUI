from TTS.api import TTS
tts = TTS(model_name="tts_models/en/ljspeech/vits")  # voz en ingles!!!!!!!
tts.tts_to_file(text="Tomas riquelme cobarde, cagon desgraciado, if you can hear this, the script is working. Horray!!!", file_path="output.wav")

